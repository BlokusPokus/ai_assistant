# Staging Environment Variables
# Copy this file to .env.stage and fill in your actual values

# Database Configuration
STAGE_DB_PASSWORD=stage_password_here
STAGE_REDIS_PASSWORD=stage_redis_password_here

# Grafana Configuration
STAGE_GRAFANA_PASSWORD=stage_admin

# API Keys (use test keys for staging)
GEMINI_API_KEY=your_gemini_api_key_here
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Security (use different keys from production)
JWT_SECRET_KEY=your_staging_jwt_secret_key_here
ENCRYPTION_KEY=your_staging_encryption_key_here

# External Services (use test credentials)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here 